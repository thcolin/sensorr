{
  "name": "tmdb",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/tmdb/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/tmdb",
        "tsConfig": "libs/tmdb/tsconfig.lib.json",
        "packageJson": "libs/tmdb/package.json",
        "main": "libs/tmdb/src/index.ts",
        "assets": ["libs/tmdb/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/tmdb/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/tmdb"],
      "options": {
        "jestConfig": "libs/tmdb/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
