{
  "npmScope": "sensorr",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e", "build-storybook"]
      }
    }
  },
  "projects": {
    "api": {
      "tags": []
    },
    "config": {
      "tags": []
    },
    "i18n": {
      "tags": []
    },
    "palette": {
      "tags": []
    },
    "sensorr": {
      "tags": []
    },
    "services": {
      "tags": []
    },
    "theme": {
      "tags": []
    },
    "tmdb": {
      "tags": []
    },
    "ui": {
      "tags": []
    },
    "utils": {
      "tags": []
    },
    "web": {
      "tags": []
    },
    "web-e2e": {
      "tags": [],
      "implicitDependencies": ["web"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
