{
  "name": "cli",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/cli/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/cli",
        "main": "apps/cli/src/main.js",
        "tsConfig": "apps/cli/tsconfig.app.json",
        "assets": [],
        "target": "node",
        "compiler": "tsc",
        "webpackConfig": "apps/cli/webpack.config.cjs",
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/cli/src/environments/environment.js",
              "with": "apps/cli/src/environments/environment.prod.js"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "cli:build",
        "inspect": false,
        "watch": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/cli/**/*.js"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/cli"],
      "options": {
        "jestConfig": "apps/cli/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
